<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".testActivity1">


    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="enter ur name and phone number"
        android:textSize="32dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-16dp"></TextView>

    <EditText
        android:id="@+id/testEnterName"
        android:layout_width="0dp"
        android:layout_height="53dp"
        android:layout_marginVertical="13dp"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:background="@drawable/roundededittext"
        app:layout_constraintBottom_toTopOf="@+id/testEnterPhoneNum"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/testEnterPhoneNum"
        android:layout_width="0dp"
        android:layout_height="53dp"
        android:layout_marginVertical="13dp"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:background="@drawable/roundededittext"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="271dp" />

    <Button
        android:id="@+id/testSubmitInfo"
        android:layout_width="231dp"
        android:layout_height="wrap_content"
        android:layout_marginVertical="40dp"
        android:layout_marginTop="48dp"
        android:backgroundTint="@color/button"
        android:fontFamily="sans-serif-black"
        android:paddingTop="13dp"
        android:paddingBottom="13dp"
        android:text="Submit Info"
        android:textSize="16dp"
        app:cornerRadius="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/testEnterPhoneNum"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>